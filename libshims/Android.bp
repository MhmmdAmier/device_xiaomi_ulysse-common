// libandroid
cc_library_shared {
    name: "libandroid_shim",
    proprietary: true,
    defaults: ["hidl_defaults"],
    srcs: [
        "libandroid_shim.cpp",
    ],
    cflags: [
        "-Werror",
        "-Wextra",
        "-Wall",
    ],
    shared_libs: [
        "libutils",
    ],
}

cc_library_shared {
    name: "libshims_dpmd",
    product_specific: true,
    srcs: [
        "libshims_dpmd.cpp",
    ]
}

cc_library_shared {
    name: "libshim_wfd",
    srcs : [
         "wfd_shim.cpp",
    ],
}

cc_library_shared {
    name: "libui_shim",

    srcs: ["GraphicBuffer.cpp"],
    shared_libs: ["libui"],

    compile_multilib: "64",

}
